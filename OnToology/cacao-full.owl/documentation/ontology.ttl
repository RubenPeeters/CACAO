                                                          "E4 Período"@pt ,
                                                          "E4 Phase"@de ,
                                                          "E4 Période"@fr ,
                                                          "E4 Περίοδος"@el ,
                                                          "E4 Пeриод"@ru ,
                                                          "E4 时期"@zh ;
